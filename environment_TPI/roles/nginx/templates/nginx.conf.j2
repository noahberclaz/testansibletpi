user www-data;
worker_processes auto;
pid /run/nginx.pid;

events {
    worker_connections 768;
}

http {
    include       /etc/nginx/mime.types;
    default_type  application/octet-stream;
    sendfile        on;
    keepalive_timeout 65;
    server {
        listen 80 default_server;
        server_name _;
        location / {
            return 200 'Nginx déployé avec Ansible !';
        }
    }
}